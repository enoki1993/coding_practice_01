{
  "name": "workspace",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "cleanup": "npx rimraf ./public ./docs",
    "copy": "xcopy /S /H /Y public\\** docs\\",
    "dev": "npm run webpack:dev && npm run webpack:server",
    "webpack:server": "npx webpack serve --config ./webpack.dev.js",
